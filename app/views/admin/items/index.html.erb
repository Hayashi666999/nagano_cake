<h1>商品一覧</h1>
<div><%= link_to "商品追加",new_admin_item_path %></div>

<table>
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
    <th>ジャンル</th>
    <th>ステータス</th>
  </tr>
  <% @items.each do |item| %>
  <tr>
    <td><%= item.id %></td>
    <td><%= item.name %></td>
    <td><%= item.price %></td>
    <td><%= item.genre_id %></td>
    <td><%= item.is_active %></td>！！
  </tr>
  <% end %>
</table>