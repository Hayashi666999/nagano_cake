<%= form_with model: Genre.new,url:admin_genres_path do |f| %>
  <h4>ジャンル名</h4>
  <%= f.text_field :name %>
  <%= f.submit '新規登録',class:"btn btn-success" %>
<% end %>

    <% @genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
        <td><%= link_to "編集する",edit_admin_genre_path(genre.id) %></td>
      </tr>
      <% end %>
